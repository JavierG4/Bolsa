  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc-watch --onSuccess \"env-cmd -f ./config/dev.env node dist/index.js\"",
    "test_mode_dev": "tsc-watch --onSuccess \"env-cmd -f ./config/test.env node dist/index.js\"",
    "test_mode": "env-cmd -f ./config/test.env node dist/index.js",
    "test": "env-cmd -f ./config/test.env vitest run",
    "test:unit": "env-cmd -f ./config/test.env vitest run src/test/*",
    "coverage": "env-cmd -f ./config/test.env vitest run --coverage --coverage.include **/src/*",
    "bd": "node dist/db/mongoose.js",
    "prueba_API": "node dist/services/alphadvantageAPI.js"
  },
  "type": "module",
  "keywords": [],
  "author": "",
