  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.5",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.0",
    "@vitest/coverage-v8": "^3.1.2",
    "env-cmd": "^10.1.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^16.0.0",
    "mongodb-memory-server": "^10.3.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "tsc-watch": "^6.3.1",
    "typedoc": "^0.28.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.1.2"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "validator": "^13.15.0"
  }
